	.wrap
		form.content-settings
			.content-settings-wrap
				.kit-form-field.settings-date(data-index='0', data-size='medium')
					.kit-form-label Даты пребывания в отеле
					.expand.kit-arrow
					input.kit-form-input.search-comeinout(readonly='readonly', type='text', name='comeinout')
				.kit-form-field(data-index='1', data-space='up', data-size='medium')
					.kit-form-label Гости
				.kit-form-field(data-space='long-up')
