		form.content-settings
			.content-settings-wrap
				+kit-form-field({
					text: "Даты пребывания в отеле",
					settings: {
						class: "settings-date",
						"data-index": "0",
						"data-size": "medium"
					},
					input: {
						name: "comeinout"
					}
				})
				+kit-form-field({
					purpose: "dropdown",
					text: "Даты пребывания в отеле",
					settings: {
						"data-index": "1",
						"data-space": "up",
						"data-size": "medium"
					}
				})
				+kit-form-field({
					purpose: "range",
					text: "Диапазон цены",
					from: "5 000",
					to: "10 000",
					note: "Стоимость за сутки пребывания в номере",
					settings: {
						"data-space": "long-up"
					},
					input: {
						name: "price-range"
					}
				})
				+kit-form-field({
					purpose: "checkbox",
					text: "Checkbox buttons",
					settings: {
						class: "kit-checkbox",
						"data-index": "2",
						"data-space": "long-up"
					},
					input: [{
						name: "smoke",
						value: 0,
						title: "Можно курить"
					}, {
						name: "pets",
						value: 1,
						title: "Можно с питомцами"
					}, {
						name: "guests",
						value: 2,
						title: "Можно пригласить гостей<br>(до 10 человек)"
					}]
				})
				+kit-form-field({
					purpose: "checkbox",
					text: "Доступность",
					settings: {
						class: "kit-checkbox",
						"data-index": "4",
					},
					input: [{
						name: "wide",
						value: 3,
						title: "Широкий коридор",
						note: "Ширина коридоров в номере<br>не менее 91 см."
					}, {
						name: "helper",
						value: 4,
						title: "Помощник для инвалидов",
						note: "На 1 этаже вас встретит специалист<br>и проводит до номера."
					}]
				})
